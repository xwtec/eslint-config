{
  "name": "@xwtec/eslint-config",
  "version": "6.1.2",
  "description": "ESLint Configuration for xwtec projects",
  "keywords": [
    "xwtec",
    "eslint",
    "config"
  ],
  "homepage": "https://github.com/xwtec/eslint-config#readme",
  "bugs": {
    "url": "https://github.com/xwtec/eslint-config/issues"
  },
  "repository": "xwtec/eslint-config",
  "license": "MIT",
  "author": {
    "name": "xwtec",
    "url": "https://github.com/xwtec"
  },
  "sideEffects": false,
  "main": "index.js",
  "files": [
    "index.js",
    "configs"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:rules": "node -r esm ./scripts/rules",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:markdown": "yarn lint:markdown --fix",
    "format:package-json": "sort-package-json \"package.json\" \"packages/*/package.json\"",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:package-json": "yarn run format:package-json --check",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s lint build test format dist",
    "test": "eslint **/*.js --config ./index.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "babel-eslint": "10.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@fisker/commitlint-config": "1.1.7",
    "@fisker/husky-config": "1.1.9",
    "@fisker/lint-staged-config": "1.0.11",
    "@xwtec/eslint-config": "6.1.2",
    "@xwtec/prettier-config": "1.0.3",
    "cz-conventional-changelog": "3.1.0",
    "eslint": "6.8.0",
    "esm": "3.2.25",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "markdownlint-cli": "0.22.0",
    "mem": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "sort-package-json": "1.40.0",
    "write-prettier-file": "1.2.5"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
