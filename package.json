{
  "name": "@xwtec/eslint-config",
  "version": "6.0.12",
  "description": "ESLint Configuration for xwtec projects",
  "main": "index.js",
  "keywords": [
    "xwtec",
    "eslint",
    "config"
  ],
  "files": [
    "index.js",
    "configs/"
  ],
  "scripts": {
    "build": "npm-run-all build:*",
    "build:rules": "node -r esm ./scripts/rules",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "release": "run-s lint build test format dist",
    "test": "eslint **/*.js --config ./index.js"
  },
  "license": "MIT",
  "dependencies": {
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "3.1.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xwtec/eslint-config.git"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@fisker/commitlint-config": "1.1.0",
    "@fisker/husky-config": "1.1.0",
    "@fisker/lint-staged-config": "1.0.4",
    "@xwtec/eslint-config": "6.0.12",
    "@xwtec/prettier-config": "1.0.2",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "6.0.1",
    "esm": "3.2.25",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "markdownlint-cli": "0.17.0",
    "mem": "5.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "write-prettier-file": "1.2.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "bugs": {
    "url": "https://github.com/xwtec/eslint-config/issues"
  },
  "homepage": "https://github.com/xwtec/eslint-config#readme",
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "prettier": "@xwtec/prettier-config",
  "sideEffects": false
}
